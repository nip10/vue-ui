{
  "name": "vue-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/vue-ui.umd.js",
  "module": "dist/vue-ui.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "prebuild": "rimraf dist",
    "build": "vite build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.vue src --fix",
    "postbuild": "terser dist/vue-ui.es.js -o dist/vue-ui.es.min.js --compress --mangle --comments /@license/i && node .scripts/post-build.mjs",
    "build:analyze": "ANALYZE=true yarn build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "changeset": "changeset",
    "changeset:status": "changeset status",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "changeset:generate": "changeset generate",
    "changeset:status:ci": "node .scripts/changeset-status.mjs"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@rushstack/eslint-patch": "^1.3.0",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/testing-library": "^0.1.1-future.2",
    "@storybook/vue3": "^7.0.18",
    "@storybook/vue3-vite": "^7.0.18",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@zag-js/checkbox": "^0.9.2",
    "@zag-js/vue": "^0.9.2",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-vue": "^9.14.1",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lucide-vue-next": "^0.233.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "storybook": "^7.0.18",
    "tailwind-merge": "^1.13.0",
    "tailwindcss": "^3.3.2",
    "terser": "^5.17.7",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.6.5"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
